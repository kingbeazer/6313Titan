                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Blog</a>
                    </li>
                    <li>
                        <a class="nav-link" href="@Url.Action("Contact", "Home")">Contact</a>
                    </li>
                    <li>
                        <a class="nav-link" href="@Url.Action("About", "Home")">About</a>
                    </li>
                </ul>

                <ul class="nav navbar-nav ml-auto">
                    @Html.Partial("_LoginPartial")
